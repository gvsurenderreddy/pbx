{% extends "VoIPUIBasicBundle::layout.html.twig" %}
{% block title %}48 | New Voicemail{% endblock %}
{% block container %}
<div class="page-header">
	<h1>New voicemail</h1>
</div>
<div class="row">
	<div class="col-md-offset-3 col-md-6">
		<form class="form-horizontal" role="form" action="{{ path('ui_company_newvoicemail', {'hash': company.hash}) }}" method="POST">
			<div class="form-group">
				<div class="col-sm-offset-2 col-sm-10">
					<button type="submit" class="btn btn-primary btn-block">Submit</button>
					<a href="{{ path('ui_company', {'hash': company.hash}) }}" class="btn btn-default btn-block">Cancel</a>
				</div>
			</div>
		</form>
	</div>
</div>

{% endblock container %}