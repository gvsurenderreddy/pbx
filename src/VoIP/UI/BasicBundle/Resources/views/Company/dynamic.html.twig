{% extends "VoIPUIBasicBundle::layout.html.twig" %}
{% block title %}48 | {{ company.name }}{% endblock %}
{% block headertitle %}Dynamic Network{% endblock headertitle %}
{% block container %}
<table class="table table-hover">
	<tr>
		<th>Created at</th>
		<th>Updated at</th>
		<th>IP</th>
		<th>Hash</th>
		<th>Token</th>
	</tr>
	{% for ip in company.dynips %}
	<tr>
		<td>{{ ip.createdat | date('d/m/Y H:i:s') }}</td>
		<td>{{ ip.updatedat | date('d/m/Y H:i:s') }}</td>
		<td>{{ ip.ip }}</td>
		<td>{{ ip.hash }}</td>
		<td>{{ ip.token }}</td>
	</tr>
	{% endfor %}
</table>
{% endblock container %}