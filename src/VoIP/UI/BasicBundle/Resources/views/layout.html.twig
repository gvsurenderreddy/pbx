			<a href="{{ path('ui_company') }}"><img src="{{ asset('images/logo.png') }}" class="main-logo"></a>
		</div>
		{% render controller('VoIPUIBasicBundle:Company:menu', {'route': app.request.attributes.get('_route')}) %}
	</div>
	<div class="col-lg-offset-2 col-md-offset-3 col-sm-offset-4 col-lg-10 col-md-9 col-sm-8">
		{% block container %}{% endblock container %}
	</div>
</div>
