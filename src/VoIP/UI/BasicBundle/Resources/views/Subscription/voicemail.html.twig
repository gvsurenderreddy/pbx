{% extends "VoIPUIBasicBundle::layout.html.twig" %}
{% block title %}48 | Edit Line{% endblock %}
{% block headertitle %}Edit Line {{ subscription.name }}{% endblock headertitle %}
{% block container %}
{{ parent() }}
<div class="row form-new">
	<div class="col-md-offset-3 col-md-6">
		<form class="form-horizontal" role="form" action="{{ path('ui_subscription_voicemail', {'hash': subscription.hash}) }}" method="POST" enctype="multipart/form-data">
			<div class="form-group">
				<label for="file" class="col-sm-2 control-label">MP3</label>
				<div class="col-sm-10">
					<input type="file" class="form-control" id="file" name="file" required>
				</div>
			</div>
			<div class="form-group">
				<div class="col-sm-offset-2 col-sm-10">
					<button type="submit" class="btn btn-danger btn-block btn-lg">Save</button>
				</div>
			</div>
		</form>
	</div>
</div>

{% endblock container %}
