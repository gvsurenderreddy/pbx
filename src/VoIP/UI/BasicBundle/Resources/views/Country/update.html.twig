{% extends "VoIPUIBasicBundle::layout_parameters.html.twig" %}
{% block title %}48 | Update countries{% endblock %}
{% block headertitle %}Update countries{% endblock headertitle %}
{% block container %}
{{ parent() }}
<div class="row form-new">
	<div class="col-md-offset-3 col-md-6">
		<form class="form-horizontal" role="form" action="{{ path('country_update') }}" method="POST" enctype="multipart/form-data">
			<div class="form-group">
				<label for="file" class="col-sm-2 control-label">JSON</label>
				<div class="col-sm-10">
					<input type="file" class="form-control" id="file" name="file" required>
				</div>
			</div>
			<div class="form-group">
				<div class="col-sm-offset-2 col-sm-10">
					<button type="submit" class="btn btn-default btn-block">Update</button>
				</div>
			</div>
		</form>
	</div>
</div>

{% endblock container %}